<!doctype html>
<html lang="en">
<head>
  <!-- HEADER_CANARY_123 -->

  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>{% block title %}{% endblock %}</title>

  <!-- Recursos externos primero -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" rel="stylesheet" />
  <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:wght@400;700&display=swap" rel="stylesheet" />

  <!-- Este marcador inserta automÃ¡ticamente /site.css y scripts de Squarespace -->
  {squarespace-headers}
</head>


<body id="{squarespace.page-id}" class="{squarespace.page-classes}">
  <header>
    {% region "header" %}{% endregion %}
  </header>

<main id="main">
  <section data-content-field="main-content">
    {% block content %}
      {squarespace.main-content}
    {% endblock %}
  </section>
</main>

  <footer>
    {% region "footer" %}{% endregion %}
  </footer>

  {squarespace-footers}

  <!-- Opcional: scripts externos -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js" defer></script>
</body>
</html>

